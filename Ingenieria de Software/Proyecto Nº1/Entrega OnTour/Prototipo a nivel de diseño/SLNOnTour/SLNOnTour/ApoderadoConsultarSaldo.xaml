                <DataGridTextColumn Binding="{x:Null}" ClipboardContentBinding="{x:Null}" Header="MONTO POR COMPLETAR"/>
            </DataGrid.Columns>
        </DataGrid>
        <Label Content="Rut" HorizontalAlignment="Left" Margin="47,28,0,0" VerticalAlignment="Top"/>
        <TextBox HorizontalAlignment="Left" Height="23" Margin="80,28,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120"/>
        <Button Content="Buscar" HorizontalAlignment="Left" Margin="219,28,0,0" VerticalAlignment="Top" Width="75"/>

    </Grid>
</Window>
